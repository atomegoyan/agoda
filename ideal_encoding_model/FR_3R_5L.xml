      </fileDesc>
      
      
      
      <!-- ENCODINGDESC -->
      <!-- Documente la relation d'un texte électronique avec sa ou ses sources -->
      <encodingDesc>
         
         <!-- Description du projet dans le cadre duquel le corpus a été compilé -->
         <projectDesc>
            
            <p xml:lang="fr"><ref target="https://..."></ref> ...</p>
            <p xml:lang="en"></p>
            
         </projectDesc>
         
         <!-- Précisions sur les pratiques et les principes éditoriaux appliqués au cours de l’encodage du texte. Plusieurs dimensions prises en compte : -->
         <editorialDecl>
            
            <!-- Corrections apportées au texte. Ex : -->
            <correction>
               <p xml:lang="fr"></p>
               <p xml:lang="en">No correction of source texts was performed.</p>
            </correction>
            
            <!--<!-\- Normalisation effectuée sur le texte source dans sa conversion vers sa forme électronique. Ex : -\->
            <normalization>
               <p xml:lang="fr"></p>
               <p xml:lang="en">Text has not been normalised, except for spacing.</p>
            </normalization>-->
            
            <!-- Résume la façon dont les traits d'union sécants de fin de ligne d’un texte source ont été traités dans sa version encodée. Ex : -->
            <hyphenation>
               <p xml:lang="fr"></p>
               <p xml:lang="en">No end-of-line hyphens were present in the source.</p>
            </hyphenation>
            
            <!-- Décrit la pratique éditoriale adoptée par rapport aux guillements dans l’original. Ex : -->
            <quotation>
               <p xml:lang="fr"></p>
               <p xml:lang="en">Quotation marks have been left in the text and are not explicitly marked up.</p>
            </quotation>
            
            <!-- Décrit les principes selon lesquels le texte a été segmenté. Ex : -->
            <segmentation>
               <p xml:lang="fr"></p>
               <p xml:lang="en">The texts are segmented into utterances (speeches) and segments (corresponding to paragraphs in the source transcription).</p>
            </segmentation>
            
         </editorialDecl>
         
         <!-- Idée de ParlaClarin : mettre un appInfo qui permet d'enregistrer des informations sur l'application qui a été utilisée pour traiter le fichier TEI -->
         <appInfo>
            <application version="111" ident="aaa">
               <label></label>
               <desc></desc>
            </application>
         </appInfo>
         
         <!-- Donne l’information détaillée sur le balisage appliqué à un document -->
         <tagsDecl>
            
            <!-- Compte des balises utilisées pour le corpus entier. À renseigner à la toute fin du processus. Ex : -->
            <!--<namespace name="http://www.tei-c.org/ns/1.0">
               <tagUsage gi="body" occurs="1"/>
               <tagUsage gi="desc" occurs="188"/>
               <tagUsage gi="div" occurs="3"/>
               <tagUsage gi="head" occurs="3"/>
               <tagUsage gi="incident" occurs="18"/>
               <tagUsage gi="note" occurs="3"/>
               <tagUsage gi="seg" occurs="595"/>
               <tagUsage gi="u" occurs="256"/>
            </namespace>-->
            
         </tagsDecl>
         
         <!-- Déclaration des classes : définition des vocabulaires contrôlés-->
         <classDecl>
            
            <!-- La taxonomie est décrite puis énumération des catégories, ces dernières sont explicitées dans l'élément catDesc -->
            <taxonomy xml:id="a">
               
               <desc xml:lang="fr">
                  <term></term>
               </desc>
               <desc xml:lang="en">
                  <term></term>
               </desc>
               
               <category xml:id="ab">
                  <catDesc xml:lang="fr">
                     <term></term>: </catDesc>
                  <catDesc xml:lang="en">
                     <term></term>: </catDesc>
               </category>
               
               <category xml:id="ac">
                  <catDesc xml:lang="fr">
                     <term></term>: </catDesc>
                  <catDesc xml:lang="en">
                     <term></term>: </catDesc>
               </category>
               
            </taxonomy>
            
            <taxonomy>
               <desc>...</desc>
               <category></category>
            </taxonomy>
            
         </classDecl>
   
      </encodingDesc>
      
      
      
      
