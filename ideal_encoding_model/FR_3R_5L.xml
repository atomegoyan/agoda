   <teiHeader>
      
      <!-- FILEDESC -->
      <!-- Description bibliographique du fichier électronique -->
      <fileDesc>
         
         <titleStmt>
            
            <!-- Titre en français et anglais -->
            <title type="main" xml:lang="fr"></title>
            <title type="main" xml:lang="en"></title>
           
            <!-- Possibilité d'un titre subordonné -->
            <title type="sub" xml:lang="fr"></title>
            <title type="sub" xml:lang="en"></title>
            
            
            <!-- Contient le titre descriptif formalisé d’une réunion ou d’une conférence, employé dans une description bibliographique pour un article provenant d'une telle réunion, ou comme le titre ou le préambule aux publications qui en émanent. -->
            <!-- @n correspond au numéro de la législature, à garder ? -> @corresp indique l'organe gouvernemental (corresp="#PO717460"), @ana pointe vers 3 taxonomies (chambre basse, législature, def de la législature)(ana="#parla.lower #parla.term") -->
            <!--<meeting n="5" >5e législature</meeting>-->
            
            
            <!-- Indique la responsabilité quant au contenu intellectuel d'une édition lorsque les éléments spécifiques relatifs aux auteurs, éditeurs, etc. ne suffisent pas ou ne s'appliquent pas -->
            <respStmt>
               <resp xml:lang="fr"></resp>
               <resp xml:lang="en"></resp>
               <persName></persName>
               <persName></persName>
            </respStmt>
            
            
            <!-- Désigne le nom d’une personne ou d’un organisme responsable du financement d’un projet ou d’un texte -->
            <funder>
               <orgName xml:lang="fr"></orgName>
               <orgName xml:lang="en"></orgName>
            </funder>
            
         </titleStmt>
         
         
         <editionStmt>
            
            <!-- Décrit les particularités de l’édition d’un texte -->
            <edition>Première édition</edition>
            
         </editionStmt>
         
         <!-- Donne infos sur la taille du corpus : discours et mots. À renseigner à la toute fin du processus. Ex : -->
         <!--<extent>
            
            <measure unit="utterances" quantity="481604" xml:lang="en">481,604 utterances</measure>
            <measure unit="words" quantity="32732720" xml:lang="en">32,732,720 words</measure>
            <measure unit="utterances" quantity="481604" xml:lang="fr">481 604 interventions</measure>
            <measure unit="words" quantity="32732720" xml:lang="fr">32 732 720 mots</measure>
            
         </extent>-->
         
         <!-- Regroupe des informations concernant la publication ou la diffusion d’un texte électronique ou d’un autre type de texte. -->
         <publicationStmt>
            
            <!-- éditeur du corpus -->
            <!-- Donne le nom de l'organisme responsable de la publication ou de la distribution d'un élément de la bibliographie -->
            <publisher xml:lang="fr"></publisher>
            <publisher xml:lang="en"></publisher>
            
           <!-- <publisher>
               <orgName xml:lang="fr"></orgName>
               <orgName xml:lang="en"></orgName>
               <ref target="https://..."></ref>
            </publisher>-->
            
            <!-- Idée de parlaClarin à la place de < publisher > : -->
            <!-- Donne le nom de la personne ou de l'organisme responsable de la publication d’un fichier électronique, autre qu’un éditeur ou un distributeur -->
            <!--<authority></authority>-->
            
            <!-- Identifiant permettant d'indiquer où trouver le corpus complet --> 
            <idno type="..." subtype="..."></idno>
            
            <!-- Licence -->
            <availability status="free">
               <licence>http://...</licence>
               <p xml:lang="fr">Description en prose de la licence.</p>
               <p xml:lang="en"></p>
            </availability>
            
            <!-- Date de parution -->
            <date when="2022-06-03"></date>
            
            <!-- Idée de parlaClarin (donne le nom d’une personne ou d’un organisme responsable de la diffusion d’un texte) -->
            <!--<distributor>CLARIN Git repository</distributor>-->
            
         </publicationStmt>
         
         <!-- Description de la source -->
         <sourceDesc>
            
            <!-- Référence bibliographique faiblement adaptée dans laquelle les sous-composants peuvent ou non être balisés -->
            <bibl>
               <!-- Titre de la source -->
               <title type="main" xml:lang="fr"></title>
               <title type="main" xml:lang="en"></title>
               
               <!--<title type="sub" xml:lang="fr"></title>
               <title type="sub" xml:lang="en"></title>-->
               
               <!-- Donne un identifiant standardisé qui peut être utilisé pour identifier une référence bibliographique (URL dont les transcriptions sont issues) -->
               <idno type="URI"></idno>
               
               <!-- Date de la transcription la plus ancienne et la plus récente, possible d'utiliser @from @to -->
               <date when="2022-06-07"></date>
            </bibl>
            
         </sourceDesc>
         
